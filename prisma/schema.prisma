datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Task {
  id        Int     @id @default(autoincrement())
  content   String
  deadline  DateTime
  priority  Int
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
